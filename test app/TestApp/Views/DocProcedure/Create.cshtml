@model test_app.Models.DocProcedure

@{
    ViewBag.Title = "Create";
    Layout = "_Layout";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("Create", "DocProcedure", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        @Html.LabelFor(model => model.DocId, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.DocId, new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.DocId, "", new { @class = "text-danger" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.ProcId, htmlAttributes: new { @class = "control-label" })
        @Html.TextBoxFor(model => model.ProcId, new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.ProcId, "", new { @class = "text-danger" })
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
}