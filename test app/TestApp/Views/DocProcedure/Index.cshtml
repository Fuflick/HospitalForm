@model test_app.Models.DocProcedure

<!DOCTYPE html>
<html>
<head>
    <title>Create DocProcedure</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>

<div class="container">
    <h2>Create DocProcedure</h2>
    <form asp-action="Create" method="post">
        <div class="form-group">
            <label asp-for="DocId" class="control-label"></label>
            <input asp-for="DocId" class="form-control" />
            <span asp-validation-for="DocId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="ProcId" class="control-label"></label>
            <input asp-for="ProcId" class="form-control" />
            <span asp-validation-for="ProcId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </form>
</div>

</body>
</html>