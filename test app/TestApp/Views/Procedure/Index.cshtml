@model IEnumerable<Procedure>   

@{
    ViewBag.Title = "Список назначеных процедур";
    Layout = "_Layout";
}

<h2>@ViewBag.Title</h2>

<table>
    <thead>
    <tr>
       <th>Название</th>
       <th colspan="2"></th> <!-- Пустая ячейка для промежутка -->
       <th>Стоимость</th>
       <th colspan="2"></th> <!-- Пустая ячейка для промежутка -->
        <th>Дата</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var procedure in Model)
            {
                <tr>
                    <td>@procedure.Body</td>
                    <td>@procedure.Cost</td>
                    <td>@procedure.Date</td>
                </tr>
            }
    </tbody>
</table> 