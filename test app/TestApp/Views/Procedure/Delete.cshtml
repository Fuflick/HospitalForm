@model test_app.Models.Procedure

<h1>Delete Procedure</h1>

<h3>Are you sure you want to delete this procedure?</h3>
<div>
    <h4>Procedure</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">ID</dt>
        <dd class="col-sm-10">@Model.Id</dd>
        <dt class="col-sm-2">Body</dt>
        <dd class="col-sm-10">@Model.Body</dd>
        <dt class="col-sm-2">Cost</dt>
        <dd class="col-sm-10">@Model.Cost</dd>
        <dt class="col-sm-2">Date</dt>
        <dd class="col-sm-10">@Model.Date</dd>
    </dl>
    <form asp-action="DeleteConfirmed" method="post">
        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Index" class="btn btn-secondary">Back to List</a>
    </form>
</div>
